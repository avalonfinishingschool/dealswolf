<%= simple_form_for(@currency) do |f| %>
  <%= f.error_notification %>

  <div class="form-group">
    <%= f.input :country, input_html: { class: 'form-control' } %>
  </div>

  <div class="form-group">
    <%= f.input :extension, input_html: { class: 'form-control' } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

